<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="adresse1" class="fr.dawan.springcore.beans.Adresse"
		primary="true">
		<constructor-arg value="1 rue montparnasse"/>
		<constructor-arg value="paris"/>
		<constructor-arg value="75001"/>
	</bean>

	<bean name="adresse2" class="fr.dawan.springcore.beans.Adresse">
		<constructor-arg value="2 rue montparnasse"/>
		<constructor-arg value="paris"/>
		<constructor-arg value="75002"/>
	</bean>

	<bean name="adresse3" class="fr.dawan.springcore.beans.Adresse">
		<constructor-arg value="3 rue montparnasse"/>
		<constructor-arg value="paris"/>
		<constructor-arg value="75003"/>
	</bean>


</beans>
